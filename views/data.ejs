<script type = "text/javascript">
    var db = openDatabase('mydb2', '1.0', 'TestDB2', 2 * 1024 * 1024);
    
    db.transaction(function (tx) {
        tx.executeSql('CREATE TABLE IF NOT EXISTS Product (Id unique, Name, Volume, Price)');
        tx.executeSql('INSERT INTO Product (id, Name, Volume, Price) VALUES (1, "CocaCola", "0.5l" ,"10.00HRK")');
        tx.executeSql('INSERT INTO PRODUCT (id, Name, Volume, Price) VALUES (2, "Sprite", "1.5L", "16.99HRK")');
        tx.executeSql('INSERT INTO PRODUCT (id, Name, Volume, Price) VALUES (3, "Vindino", "0.5l" ,"6.00HRK")');
        tx.executeSql('INSERT INTO PRODUCT (id, Name, Volume, Price) VALUES (4, "Fanta", "1L", "8.50HRK")');
        tx.executeSql('INSERT INTO PRODUCT (id, Name, Volume, Price) VALUES (5, "JackDaniels", "0.7l" ,"80.00HRK")');
        tx.executeSql('INSERT INTO PRODUCT (id, Name, Volume, Price) VALUES (6, "Pivo", "0.5L", "10.99HRK")');
    })


    db.transaction(function (tx) {
        tx.executeSql('SELECT * FROM PRODUCT', [], function (tx, results) {
            var len = results.rows.length, i;
            let rows = "";


            for (i = 0; i < len; i++) {
                rows += "<tr><td>" + results.rows.item(i).Id +"</td><td>"+ results.rows.item(i).Name+"</td><td>"+ results.rows.item(i).Price+"</td>" + "</tr>";
            }   
            let table = "<table style=\"border:solid 1px black\"><tr><th>Id</th><th>Name</th><th>Price</th></tr>"+ rows+"</table>"

            document.getElementById("data").innerHTML = table;

        }, null);
    }); 

    function InjectionOn() {

        let productName = document.getElementById("injectionOnInput").value    
        
        if(productName!=""){
            let words = productName.split(" ");
            
            let keywords = ['OR', 'AND', 'UNION', 'JOIN', 'NOT', 'NULL' ,'NOT NULL' ,'WHERE' ,'IS' ,'LIKE']


            let input = "";
            flag = false;
            
            for(let i = 0; i < words.length; ++i){

                if(keywords.includes(words[i].toUpperCase())){
                    input+= words[i] + " "
                }else{
                    if((words[i].match(/^[0-9a-zA-Z]+[a-zA-Z0-9]*$/))){
                        
                            if(!flag){
                                input+="'"+words[i]+"' ";
                                flag = true;
                            }else{
                                input+="Name = '"+words[i]+"' ";
                            }


                    }else{
                        input+= words[i] + " "
                    }
                }
            }

            let sqlQuery = "SELECT * FROM PRODUCT WHERE Name =" + input; 

            db.transaction(function (tx) {
                tx.executeSql(sqlQuery, [], function (tx, results) {
                    var len = results.rows.length, i;
                    let rows = "";
        
        
                    for (i = 0; i < len; i++) {
                        rows += "<tr><td>" + results.rows.item(i).Id +"</td><td>"+ results.rows.item(i).Name+"</td><td>"+ results.rows.item(i).Price+"</td>" + "</tr>";
                    }   
                    let table = "<table style=\"border:solid 1px black\"><tr><th>Id</th><th>Name</th><th>Price</th></tr>"+ rows+"</table>"
        
                    document.getElementById("queryResult").innerHTML = table;
    
                }, null);
            }); 
        }else{
            alert("Empty input is not valid")
        }
      }

    function InjectionOff() {

        let productName = document.getElementById("injectionOffInput").value    

        let sqlQuery = "", input = "";


        if(productName!=""){
            let words = productName.split(" ");
            
            let keywords = ['OR', 'AND', 'UNION', 'JOIN', 'NOT', 'NULL' ,'NOT NULL' ,'WHERE' ,'IS' ,'LIKE']

            let flag = false;
    
            for(let i = 0; i < words.length; ++i){

                let word = words[i];

                if(keywords.includes(words[i].toUpperCase())){
                    input+= words[i] + " "
                }else{
                    if(word.match(/^[a-zA-Z]+$/)){

                        if(!flag){
                            input+="'"+words[i]+"' ";
                            flag = true;
                        }else{
                            input+="Name = '"+words[i]+"' ";
                        }
                       sqlQuery = "SELECT * FROM PRODUCT WHERE Name =" + input; 
                    }else{
                        alert("Invalid input")
                        return;
                    }
                }
            }
        }else{
            alert("Empty input is not permitted")
            return;
        }

        db.transaction(function (tx) {
            tx.executeSql(sqlQuery, [], function (tx, results) {
                var len = results.rows.length, i;
                let rows = "";
    
    
                for (i = 0; i < len; i++) {
                    rows += "<tr><td>" + results.rows.item(i).Id +"</td><td>"+ results.rows.item(i).Name+"</td><td>"+ results.rows.item(i).Price+"</td>" + "</tr>";
                }   
                let table = "<table style=\"border:solid 1px black\"><tr><th>Id</th><th>Name</th><th>Price</th></tr>"+ rows+"</table>"
    
                document.getElementById("queryResult").innerHTML = table;

            }, null);
        }); 
      }

    function toggleSQLInjection(){
        if (document.getElementById('sqlCheckbox').checked) {
            document.getElementById("injectionOn").style.display ="block";
            document.getElementById("injectionOff").style.display ="none";
        }else{
            document.getElementById("injectionOn").style.display ="none";
            document.getElementById("injectionOff").style.display ="block";
        }
    }

    function XSS(){

        let input = document.getElementById('xssInput').value;

        let imageElement = document.createElement("img");
        imageElement.style.height = 200;

        let url = "";

        if (document.getElementById('xssCheckbox').checked) {
            url = input;
            
            // Web preglednici imaju zaštitu protiv XSS-a. Kod je testiran kad se isključi zaštita i sve radi. 
            // Ako želite testirati, isključite flag u skripti google Chroma prema uputama na sljedećem linku.
            // https://stackoverflow.com/questions/12926997/how-to-temporarily-disable-xss-protection-in-modern-browsers-for-testing
            
            alert(document.cookie)
        }else{
            url = input.escape()
        }

       
        imageElement.src = url;

        let e = document.createElement("div");
        e.className = "img";
        e.style.height = '200px';
        e.appendChild(imageElement);

        document.getElementById("imageHolder").appendChild(e);
    }


    String.prototype.escape = function() {
        var tagsToReplace = {
            '&': '&amp;',
            '<': '&lt;',
            '>': '&gt;'
        };
        return this.replace(/[&<>]/g, function(tag) {
            return tagsToReplace[tag] || tag;
        });
    };
</script> 